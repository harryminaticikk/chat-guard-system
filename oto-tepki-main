client.on('message', async (msg) => {
    if(!msg.guild) return;
    if(msg.channel.id === "kanalid")  await msg.react('👍')
    if(msg.channel.id === "kanalid")  await msg.react('👎')
    if(msg.channel.id === "kanalid")  await msg.react('👍')
    if(msg.channel.id === "kanalid")  await msg.react('👎')
    if(msg.channel.id === "kanalid")  await msg.react('👍')
    if(msg.channel.id === "kanalid")  await msg.react('👎')
    if(msg.channel.id === "kanalid")  await msg.react('👍')
    if(msg.channel.id === "kanalid")  await msg.react('👎')
    });
